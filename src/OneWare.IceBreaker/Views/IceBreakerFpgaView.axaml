        </Border>

        <Grid Grid.Row="1" Grid.Column="2" Height="190">

            <Border Background="#2F4362" Margin="0 50" />

            <Border CornerRadius="50" Height="65" VerticalAlignment="Top"
                    Background="{DynamicResource ThemeBackgroundBrush}" />
            <Border CornerRadius="50" Height="40" Background="{DynamicResource ThemeBackgroundBrush}" />
            <Border CornerRadius="50" Height="65" VerticalAlignment="Bottom"
                    Background="{DynamicResource ThemeBackgroundBrush}" />
        </Grid>

        <Border Grid.Row="1" Grid.Column="3" Background="#2F4362" Width="100" Height="180" CornerRadius="10">
