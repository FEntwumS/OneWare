            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Classes="WindowButtons">
                <controls:Spinner IsVisible="{Binding IsLoading}" Height="30" />
                <Button Command="{Binding Close}"
                        CommandParameter="{Binding $self}">Close</Button>
                <Button Background="{DynamicResource ThemeAccentBrush}"
                        Command="{Binding InstallPluginAsync}" 
                        CommandParameter="{Binding $self}">Install now</Button>
            </StackPanel>
        </Panel>
