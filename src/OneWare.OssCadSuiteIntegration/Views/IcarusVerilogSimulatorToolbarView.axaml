<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:OneWare.OssCadSuiteIntegration.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OneWare.OssCadSuiteIntegration.Views.IcarusVerilogSimulatorToolbarView"
             x:DataType="viewModels:IcarusVerilogSimulatorToolbarViewModel">
    <StackPanel Classes="HToolBar" DockPanel.Dock="Left">
        <TextBlock Text="IVerilog Arguments:" />
        <TextBox Classes="ToolBar" Width="200" Text="{Binding IcarusVerilogArguments}"/>
    </StackPanel>
</UserControl>
